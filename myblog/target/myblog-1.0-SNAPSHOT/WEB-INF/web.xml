<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>servlets.register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>judge</servlet-name>
    <servlet-class>servlets.judge</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/servlets/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>judge</servlet-name>
    <url-pattern>/servlets/judge</url-pattern>
  </servlet-mapping>
</web-app>
